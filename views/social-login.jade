doctype html
html
	head
		meta(charset='utf8')
		meta(http-equiv='X-UA-Compatible' , content='IE=edge')
		meta(name='viewport' , content='initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no, width=device-width')
		title(ng-bind-template="{{thisCompany.name}} - Acquisitions") Acquisitions
		link(rel='shortcut icon' , href='img/favicon.png')
		link(href='/css/style.css' , rel='stylesheet')
		link(href='https://fonts.googleapis.com/css?family=Lato:300,400,500,600,700,800,900|family=Poppins:700' , rel='stylesheet')
		base(href='/')
	body(ng-app='acquibaseApp' , ng-controller='authController' , ng-class="{'disable-scroll': disableScroll}")
		{{socialLogin()}}
		script(defer src='libs/angular/angular.js')
		script(defer src='libs/angular-route/angular-route.min.js')
		script(defer src='libs/angular-cookies/angular-cookies.min.js')
		script(defer src='libs/d3/d3.min.js')
		script(defer src='js/app.js')
		script(defer src='js/filters.js')
		script(defer src='js/directives/searchbar.js')
		script(defer src='js/directives/navaccounts.js')
		script(defer src='js/controllers/acquibaseController.js')
		script(defer src='js/services/acquibaseFactory.js')
		script(defer src='js/services/profileFactory.js')
		script(defer src="js/services/authFactory.js")
		script(defer src="js/services/dataFactory.js")
